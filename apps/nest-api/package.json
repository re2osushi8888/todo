    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.11.9",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "@vitest/coverage-v8": "^3.0.9",
    "globals": "^16.0.0",
    "source-map-support": "^0.5.21",
